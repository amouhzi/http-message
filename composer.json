{
  "name": "http-message/http-message",
  "description": "PSR HTTP Message implementations",
  "type": "library",
  "license": "BSD-2-Clause",
  "keywords": [
    "http",
    "psr",
    "psr-7"
  ],
  "homepage": "https://github.com/amouhzi/http-message",
  "support": {
    "issues": "https://github.com/amouhzi/http-message/issues",
    "source": "https://github.com/amouhzi/http-message"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "require": {
    "php": ">=5.4",
    "psr/http-message": "~1.0"
  },
  "require-dev": {
    "phpunit/PHPUnit": "~4.6",
    "squizlabs/php_codesniffer": "^2.3.1"
  },
  "provide": {
    "psr/http-message-implementation": "~1.0.0"
  },
  "autoload": {
    "psr-4": {
      "HttpMessage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HttpMessageTest\\": "test/"
    },
    "files": [
      "test/TestAsset/Functions.php",
      "test/TestAsset/SapiResponse.php"
    ]
  }
}
